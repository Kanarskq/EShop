@model dynamic

<h1>User Profile</h1>

<div>
    <h4>@Model.Name</h4>
    <dl class="row">
        <dt class="col-sm-2">Email</dt>
        <dd class="col-sm-10">@Model.EmailAddress</dd>
        <dt class="col-sm-2">Profile Image</dt>
        <dd class="col-sm-10">
            <img src="@Model.ProfileImage" alt="Profile Image" />
        </dd>
        <dt class="col-sm-2">Roles</dt>
        <dd class="col-sm-10">
            @foreach (var role in Model.Roles)
            {
                <span>@role</span>
            }
        </dd>
    </dl>
</div>